	<queryString>
		<![CDATA[SELECT learner.first_name AS learner_name,
   learner.last_name AS learner_surname,
   learner.passport_number AS learner_passport_num,
   learner.rsa_id_number AS learner_id_num,
   learner.cell_number AS learner_cell,
   learnerTitle.description AS learner_title,
   contact_person.first_name AS contact_person_name,
   contact_person.last_name AS contact_person_surname,
   company.company_guid AS guid, company.company_name  AS company_name,
   company.levy_number ,company.tel_number  AS company_tel,company.fax_number AS company_fax,
   company.email AS company_email,company.trading_name,
   assessorModerator.first_name AS AM_Name,assessorModerator.last_name AS AM_lastname,assessorModerator.rsa_id_number AS AM_ID,
   assessorModerator.passport_number AS AM_Passport_num,assessorModeratorTitle.description AS AM_Title

FROM merseta.learners AS learner


LEFT JOIN merseta.users AS contact_person
ON contact_person.id=$P{contact_person_id}

LEFT JOIN merseta.users AS assessorModerator
ON assessorModerator.id=$P{assessor_moderator_id}

LEFT JOIN merseta.title AS assessorModeratorTitle
ON assessorModeratorTitle.id=assessorModerator.title_id


LEFT JOIN merseta.title AS learnerTitle
ON learnerTitle.id=4 /**learner.title_id*/

LEFT JOIN merseta.company AS company
ON company.id=$P{company_id}


WHERE learner.id=$P{learner_id}]]>
	</queryString>
