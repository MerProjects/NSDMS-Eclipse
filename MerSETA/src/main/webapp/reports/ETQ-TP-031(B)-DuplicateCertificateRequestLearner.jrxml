	<queryString language="SQL">
		<![CDATA[SELECT  learner.first_name,learner.last_name,
learner.passport_number AS learner_passport_num,
learner.rsa_id_number AS learner_id_num,postalAddress.address_line_1,
postalAddress.address_line_2,postalAddress.address_line_3,postalAddress.address_line_4,postalAddress.postcode,
town.description AS postal_add_town

FROM merseta.learners AS learner

LEFT JOIN merseta.address AS postalAddress
ON postalAddress.id=learner.postal_address_id

LEFT JOIN merseta.towns AS town
ON town.id=postalAddress.town_id

WHERE learner.id=$P{learner_id}]]>
	</queryString>
