	<queryString>
		<![CDATA[SELECT learner.first_name AS learner_name,
   learner.last_name AS learner_surname,
   learner.passport_number AS learner_passport_num,
   learner.cell_number AS learner_cell, 
   learner.rsa_id_number AS learner_id_num,
   company.company_guid AS guid, company.company_name  AS company_name,
   company.levy_number ,company.tel_number  AS company_tel,company.fax_number AS company_fax,
   company.email AS company_email,
   
   address.address_line_1 address_line_one,
   address.address_line_2 address_line_two,
   address.address_line_3 address_line_three,
   address.address_line_4 address_line_four,
   towns.description AS townDesc,
   address.postcode As postal_code,
   
   learner_address.address_line_1 AS learner_address_line_one,
   learner_address.address_line_2 AS learner_address_line_two,
   learner_address.address_line_3 AS learner_address_line_three,
   learner_address.address_line_4 AS learner_address_line_four,
   learner_towns.description AS learner_townDesc,
   learner_address.postcode As learner_postal_code

FROM merseta.learners AS learner


LEFT JOIN merseta.company AS company
ON company.id=$P{company_id}

LEFT JOIN merseta.address address
ON company.postal_address_id = address.id

LEFT JOIN merseta.address learner_address
ON learner.postal_address_id = learner_address.id

LEFT JOIN merseta.towns AS towns
ON  address.town_id=towns.id

LEFT JOIN merseta.towns AS learner_towns
ON learner_address.town_id=learner_towns.id

WHERE learner.id=$P{learner_id}]]>
	</queryString>
