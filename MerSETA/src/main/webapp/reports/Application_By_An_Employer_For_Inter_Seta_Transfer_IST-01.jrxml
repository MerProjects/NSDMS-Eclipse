	<queryString>
		<![CDATA[SELECT intersetatrans.action_date AS actiondate,
    
    company.company_name AS copmanyname,
    company.trading_name AS tradingname,
    company.levy_number As levynumber,
    company.tel_number  AS telnumber,
    company.fax_number AS faxnumber,
    company.email      AS email,
    
    siccode.code AS code,
    
    etqa.description AS currentEtqaDescription,
    etqa2.description AS newEtqaDescription

FROM merseta.inter_seta_transfer AS intersetatrans

LEFT JOIN merseta.company AS company
ON company.id = intersetatrans.company_id

LEFT JOIN merseta.sic_code AS siccode
ON company.sic_code_id = siccode.id

LEFT JOIN merseta.etqa AS etqa
ON etqa.id = intersetatrans.current_etqa_id

LEFT JOIN merseta.etqa AS etqa2
ON etqa2.id = intersetatrans.new_etqa_id

WHERE intersetatrans.id = $P{inter_seta_transfer_id}]]>
	</queryString>
