	<queryString language="SQL">
		<![CDATA[SELECT
	   ((SUM(COALESCE(mandatory_grant.quantity,0)-(COALESCE(mandatory_grant.african_female_disability,0) + 
		COALESCE(mandatory_grant.african_male_disability,0) + 
		COALESCE(mandatory_grant.coloured_female_disability,0) +
		COALESCE(mandatory_grant.coloured_male_disability,0) +                                        
		COALESCE(mandatory_grant.indian_female_disability,0) + 
		COALESCE(mandatory_grant.indian_male_disability,0) + 
		COALESCE(mandatory_grant.white_female_disability,0) +
		COALESCE(mandatory_grant.white_male_disability,0))) * COALESCE(intervention_type.basic_grant_amount,0)) +
		
	    (SUM(COALESCE(mandatory_grant.african_female_disability,0) + 
		COALESCE(mandatory_grant.african_male_disability,0) + 
		COALESCE(mandatory_grant.coloured_female_disability,0) +
		COALESCE(mandatory_grant.coloured_male_disability,0) + 
		COALESCE(mandatory_grant.indian_female_disability,0) + 
		COALESCE(mandatory_grant.indian_male_disability,0) + 
		COALESCE(mandatory_grant.white_female_disability,0) +
		COALESCE(mandatory_grant.white_male_disability,0)) * COALESCE(intervention_type.disability_grant_amount,0))) AS TotalFundingValue
  
 FROM   merseta.mandatory_grant  mandatory_grant
 
 LEFT JOIN  merseta.intervention_type AS intervention_type
 ON intervention_type.id=mandatory_grant.intervention_type_id

 LEFT OUTER JOIN merseta.etqa etqa 
 ON  mandatory_grant.etqa_id=etqa.id 
 WHERE mandatory_grant.wsp_id=$P{wsp_id} 
 AND  mandatory_grant.wsp_report=$P{wsp_report}
 AND (etqa.code=$P{etqa_code} OR  mandatory_grant.funding_id=$P{funding_id})
 
 GROUP BY merseta.intervention_type.basic_grant_amount, merseta.intervention_type.disability_grant_amount]]>
	</queryString>
