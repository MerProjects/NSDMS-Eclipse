					<textElement textAlignment="Justified" verticalAlignment="Top" markup="html">
						<font size="7.5"/>
						<paragraph lineSpacing="Single"/>
					</textElement>
					<textFieldExpression><![CDATA["A failure to furnish any of the information, documentation or reports required in terms of this Agreement and the merSETA ETQA procedures;"]]></textFieldExpression>
				</textField>
				<textField>
					<reportElement x="19" y="289" width="237" height="5" uuid="360ce984-221d-4ac4-9883-1216a816afbf"/>
