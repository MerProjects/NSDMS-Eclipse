					<textElement>
						<font size="7.5"/>
						<paragraph lineSpacing="Single"/>
					</textElement>
					<textFieldExpression><![CDATA["20.1.6.3"]]></textFieldExpression>
				</textField>
				<textField isStretchWithOverflow="true">
					<reportElement x="54" y="329" width="202" height="50" uuid="eedcdf67-f113-4eed-85f4-ac6a541d3b9c"/>
					<textElement textAlignment="Justified" verticalAlignment="Top" markup="html">
						<font size="7.5"/>
						<paragraph lineSpacing="Single"/>
					</textElement>
					<textFieldExpression><![CDATA["A failure to allow the merSETA or its appointed financial officer, accountant, auditor or other suitably qualified person to inspect the Organisation's financial statements or books of records related to the merSETA funded Discretionary Grant Programme; or "]]></textFieldExpression>
				</textField>
				<textField>
					<reportElement x="19" y="324" width="237" height="5" uuid="a2d7df9d-d43d-477d-a42a-f06160099f54"/>
					<textElement textAlignment="Justified" markup="html">
						<font size="7.5"/>
						<paragraph lineSpacing="Single"/>
					</textElement>
				</textField>
				<textField isStretchWithOverflow="true">
					<reportElement x="19" y="384" width="35" height="10" uuid="32cd611a-0351-4286-b5ab-22db0b44fe93"/>
					<textElement>
						<font size="7.5"/>
						<paragraph lineSpacing="Single"/>
					</textElement>
					<textFieldExpression><![CDATA["20.1.6.4"]]></textFieldExpression>
				</textField>
				<textField isStretchWithOverflow="true">
					<reportElement x="54" y="384" width="202" height="10" uuid="aa0cde31-5246-4009-81df-8a0bd6b7df20"/>
					<textElement textAlignment="Justified" verticalAlignment="Top" markup="html">
						<font size="7.5"/>
						<paragraph lineSpacing="Single"/>
